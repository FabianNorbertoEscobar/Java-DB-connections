<hibernate-mapping>
    <class name="app.Persona" table="Persona">
        <id column="dni" name="dni" type="int" />
        <property column="nombre" name="nombre" type="string" />
        <property column="apellido" name="apellido" type="string" />
        <property column="estatura" name="estatura" type="float" />
        <property column="masa" name="masa" type="float" />
    </class>
</hibernate-mapping>